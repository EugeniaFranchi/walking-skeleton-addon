{
    "manifest_version": 2,
    "name": "wasmaddon",
    "version": "1.0",
    "description": "Adds a red border to all webpages matching mozilla.org.",
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.mozilla.org/*",
                "*://*.google.com/*"
            ],
            "js": [
                "pkg/wasmaddon.js",
                "www/dist/bootstrap.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "pkg/wasmaddon_bg.wasm",
        "classifier/model/model.json"
    ]
}